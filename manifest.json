{
    "manifest_version": 3,
    "name": "Boriiing",
    "version": "1.0",
    "description": "The only heterophobic extension you need",
    "author": "rainbows&unicorns",

    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },

    "content_scripts": [
        {
          "matches": ["*://*/*"],
          "js": ["scripts/content.js"],
          "all_frames": true,
          "run-at": "document_end"
        }
      ]
  }